programa
{
	
	funcao inicio()
	{
		escreva("Escreva o preço do litro da gasolina: R$ ")
		real numeroUm
		leia(numeroUm)
		escreva("Qantos litros foram vendidos ao cliente?: ")
		real numeroDois
		leia(numeroDois)
		real multiplicacao = numeroUm * numeroDois
		escreva("O preço a se pagar é R$ ",multiplicacao)
	}
}
