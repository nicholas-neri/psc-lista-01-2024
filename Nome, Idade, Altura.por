programa 
{
  funcao inicio() {
        escreva("Digite seu nome:\n")
        cadeia nome
        leia(nome)
        escreva("Informe sua idade\n")
        inteiro idade
        leia(idade)
        escreva("Altura\n")
        real altura
        leia(altura)
        escreva("Nome: ", nome, "\n", "Idade: ", idade, "\n", "Altura: ", altura)
  }
}
