programa
{
	
	funcao inicio()
	{
		escreva("Digite a 1 nota: ")
		inteiro numeroUm
		leia(numeroUm)
		escreva("Digite a 2 nota: ")
		inteiro numeroDois
		leia(numeroDois)
		escreva("Digite a 3 nota: ")
		inteiro numeroTres
		leia(numeroTres)
		escreva("Digite a 4 nota: ")
		inteiro numeroQuatro
		leia(numeroQuatro)
		inteiro media = (numeroUm + numeroDois + numeroTres + numeroQuatro) / 4
		escreva("A média dos números é ",media)
	}
}
