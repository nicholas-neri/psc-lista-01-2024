programa
{
	
	funcao inicio()
	{
		escreva("Digite o 1 número: ")
		inteiro numeroUm
		leia(numeroUm)
		escreva("Digite o 2 número: ")
		inteiro numeroDois
		leia(numeroDois)
		inteiro soma = numeroUm + numeroDois
		escreva ("A soma dos números é ",soma)
		inteiro subtracao = numeroUm - numeroDois
		escreva (" A subtração dos números é ",subtracao)
		inteiro multiplicacao = numeroUm * numeroDois
		escreva (" A multiplicação dos números é ",multiplicacao)
		
	}
}
